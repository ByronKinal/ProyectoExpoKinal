	<queryString language="SQL">
		<![CDATA[SELECT cajerodb2.facturas.total,
	cajerodb2.facturas.`idFactura`,
	cajerodb2.facturas.`metodoPago`,
	cajerodb2.usuarios.`nombreUsuario`,
	cajerodb2.pagos.cantidad
FROM cajerodb2.facturas
	INNER JOIN cajerodb2.usuarios ON 
	 cajerodb2.facturas.`idEmpleado` = cajerodb2.usuarios.`idUsuario` 
	INNER JOIN cajerodb2.pagos ON 
	 cajerodb2.facturas.`idPago` = cajerodb2.pagos.`idPago` 
WHERE 
	 cajerodb2.facturas.`idCompra` = $P{idCompra}]]>
